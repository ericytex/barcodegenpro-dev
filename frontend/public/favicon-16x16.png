<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 16 16" width="16" height="16">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e40af;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#60a5fa;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="16" height="16" fill="url(#bg)" rx="3"/>
  
  <!-- Main barcode pattern -->
  <rect x="1" y="2" width="1" height="12" fill="#ffffff"/>
  <rect x="3" y="2" width="1" height="12" fill="#ffffff"/>
  <rect x="5" y="2" width="1" height="12" fill="#ffffff"/>
  <rect x="7" y="2" width="2" height="12" fill="#ffffff"/>
  <rect x="10" y="2" width="1" height="12" fill="#ffffff"/>
  <rect x="12" y="2" width="1" height="12" fill="#ffffff"/>
  <rect x="14" y="2" width="1" height="12" fill="#ffffff"/>
  
  <!-- QR code pattern in top-left -->
  <rect x="0" y="0" width="2" height="2" fill="#ffffff" opacity="0.9"/>
  <rect x="3" y="0" width="1" height="1" fill="#ffffff" opacity="0.9"/>
  <rect x="0" y="3" width="1" height="1" fill="#ffffff" opacity="0.9"/>
  <rect x="2" y="3" width="1" height="1" fill="#ffffff" opacity="0.9"/>
</svg>
