<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="32" height="32">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e40af;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#60a5fa;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="barcode" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f8fafc;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background with rounded corners -->
  <rect width="32" height="32" fill="url(#bg)" rx="7"/>
  
  <!-- Main barcode pattern (CODE128 style) -->
  <rect x="2" y="4" width="1" height="24" fill="url(#barcode)"/>
  <rect x="4" y="4" width="2" height="24" fill="url(#barcode)"/>
  <rect x="7" y="4" width="1" height="24" fill="url(#barcode)"/>
  <rect x="9" y="4" width="3" height="24" fill="url(#barcode)"/>
  <rect x="13" y="4" width="1" height="24" fill="url(#barcode)"/>
  <rect x="15" y="4" width="2" height="24" fill="url(#barcode)"/>
  <rect x="18" y="4" width="1" height="24" fill="url(#barcode)"/>
  <rect x="20" y="4" width="2" height="24" fill="url(#barcode)"/>
  <rect x="23" y="4" width="1" height="24" fill="url(#barcode)"/>
  <rect x="25" y="4" width="2" height="24" fill="url(#barcode)"/>
  <rect x="28" y="4" width="1" height="24" fill="url(#barcode)"/>
  
  <!-- QR code pattern in top-left corner -->
  <rect x="1" y="1" width="3" height="3" fill="#ffffff" opacity="0.9"/>
  <rect x="5" y="1" width="1" height="1" fill="#ffffff" opacity="0.9"/>
  <rect x="7" y="1" width="1" height="1" fill="#ffffff" opacity="0.9"/>
  <rect x="1" y="5" width="1" height="1" fill="#ffffff" opacity="0.9"/>
  <rect x="3" y="5" width="1" height="1" fill="#ffffff" opacity="0.9"/>
  <rect x="5" y="5" width="1" height="1" fill="#ffffff" opacity="0.9"/>
  <rect x="1" y="7" width="3" height="1" fill="#ffffff" opacity="0.9"/>
  <rect x="5" y="7" width="1" height="1" fill="#ffffff" opacity="0.9"/>
  <rect x="7" y="7" width="1" height="1" fill="#ffffff" opacity="0.9"/>
  
  <!-- Small decorative elements -->
  <circle cx="29" cy="2" r="1" fill="#ffffff" opacity="0.6"/>
  <circle cx="30" cy="30" r="1" fill="#ffffff" opacity="0.4"/>
</svg>
